{
  "version": 2,
  "projects": {
    "cli": {
      "root": "libs/cli",
      "sourceRoot": "libs/cli/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/cli",
            "tsConfig": "libs/cli/tsconfig.lib.json",
            "packageJson": "libs/cli/package.json",
            "main": "libs/cli/src/index.ts",
            "assets": ["libs/cli/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/cli/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/cli"],
          "options": {
            "jestConfig": "libs/cli/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/core",
            "main": "libs/core/src/index.ts",
            "packageJson": "libs/core/package.json",
            "tsConfig": "libs/core/tsconfig.lib.json",
            "assets": ["libs/core/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "plugin-angular": {
      "root": "libs/plugin/angular",
      "sourceRoot": "libs/plugin/angular/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/plugin/angular/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/plugin/angular"],
          "options": {
            "jestConfig": "libs/plugin/angular/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/plugin/angular",
            "tsConfig": "libs/plugin/angular/tsconfig.lib.json",
            "packageJson": "libs/plugin/angular/package.json",
            "main": "libs/plugin/angular/src/index.ts",
            "assets": [
              "libs/plugin/angular/*.md",
              {
                "input": "./libs/plugin/angular/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./libs/plugin/angular/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./libs/plugin/angular",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./libs/plugin/angular",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "plugin-angular-e2e": {
      "root": "apps/plugin/angular-e2e",
      "projectType": "application",
      "sourceRoot": "apps/plugin/angular-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "plugin-angular:build",
            "npmPackageName": "@nx-clean/angular",
            "pluginOutputPath": "dist/libs/plugin/angular",
            "jestConfig": "apps/plugin/angular-e2e/jest.config.js"
          }
        }
      }
    },
    "plugin-core": "libs/plugin/core",
    "plugin-core-e2e": "apps/plugin/core-e2e",
    "todo-angular": {
      "projectType": "application",
      "root": "apps/todo/angular",
      "sourceRoot": "apps/todo/angular/src",
      "prefix": "todo",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/todo/angular",
            "index": "apps/todo/angular/src/index.html",
            "main": "apps/todo/angular/src/main.ts",
            "polyfills": "apps/todo/angular/src/polyfills.ts",
            "tsConfig": "apps/todo/angular/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/todo/angular/src/favicon.ico",
              "apps/todo/angular/src/assets"
            ],
            "styles": ["apps/todo/angular/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/todo/angular/src/environments/environment.ts",
                  "with": "apps/todo/angular/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "todo-angular:build:production"
            },
            "development": {
              "browserTarget": "todo-angular:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/todo/angular/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "todo-angular:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/todo/angular/src/**/*.ts",
              "apps/todo/angular/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/todo/angular"],
          "options": {
            "jestConfig": "apps/todo/angular/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todo-angular-e2e": {
      "root": "apps/todo/angular-e2e",
      "sourceRoot": "apps/todo/angular-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/todo/angular-e2e/cypress.json",
            "tsConfig": "apps/todo/angular-e2e/tsconfig.e2e.json",
            "devServerTarget": "todo-angular:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "todo-angular:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/todo/angular-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "todo-data-access": {
      "root": "libs/todo/data-access",
      "sourceRoot": "libs/todo/data-access/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/todo/data-access",
            "main": "libs/todo/data-access/src/index.ts",
            "packageJson": "libs/todo/data-access/package.json",
            "tsConfig": "libs/todo/data-access/tsconfig.lib.json",
            "assets": ["libs/todo/data-access/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/todo/data-access/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/todo/data-access"],
          "options": {
            "jestConfig": "libs/todo/data-access/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todo-domain": {
      "root": "libs/todo/domain",
      "sourceRoot": "libs/todo/domain/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/todo/domain",
            "main": "libs/todo/domain/src/index.ts",
            "packageJson": "libs/todo/domain/package.json",
            "tsConfig": "libs/todo/domain/tsconfig.lib.json",
            "assets": ["libs/todo/domain/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/todo/domain/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/todo/domain"],
          "options": {
            "jestConfig": "libs/todo/domain/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todo-nest": {
      "root": "apps/todo/nest",
      "sourceRoot": "apps/todo/nest/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/todo/nest",
            "main": "apps/todo/nest/src/main.ts",
            "tsConfig": "apps/todo/nest/tsconfig.app.json",
            "assets": ["apps/todo/nest/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/todo/nest/src/environments/environment.ts",
                  "with": "apps/todo/nest/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "todo-nest:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/todo/nest/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/todo/nest"],
          "options": {
            "jestConfig": "apps/todo/nest/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todo-presentation": {
      "root": "libs/todo/presentation",
      "sourceRoot": "libs/todo/presentation/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/todo/presentation",
            "main": "libs/todo/presentation/src/index.ts",
            "packageJson": "libs/todo/presentation/package.json",
            "tsConfig": "libs/todo/presentation/tsconfig.lib.json",
            "assets": ["libs/todo/presentation/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/todo/presentation/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/todo/presentation"],
          "options": {
            "jestConfig": "libs/todo/presentation/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "todo-angular"
}
